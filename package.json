{
  "name": "components-seed",
  "version": "0.0.2",
  "type": "module",
  "main": "./bundle/index.cjs.js",
  "module": "./bundle/index.es.js",
  "types": "./bundle/index.d.ts",
  "files": [
    "bundle"
  ],
  "eslintConfig": {
    "extends": "./eslint.config.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "generate:types": "ts-node ./src/scripts/generate-types-index.ts",
    "generate:utils": "ts-node ./src/scripts/generate-utils-index.ts",
    "generate:components": "ts-node ./src/scripts/generate-components-index.ts",
    "generate-indexes": "ts-node ./src/scripts",
    "bundle": "npm run generate-indexes && tsc -p tsconfig.app.json && vite build && cp package.json bundle/"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/glob": "^9.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vite-plugin-dts": "^4.5.4"
  }
}
