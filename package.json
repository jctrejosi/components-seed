{
  "name": "components-seed",
  "version": "0.0.8",
  "type": "module",
  "main": "./bundle/index.cjs.js",
  "module": "./bundle/index.es.js",
  "types": "./bundle/index.d.ts",
  "files": [
    "bundle",
    ".prettierrc",
    "eslint.config.js"
  ],
  "eslintConfig": {
    "extends": "./eslint.config.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "format": "prettier --write .",
    "generate:defaults": "ts-node ./scripts/generate-defaults.ts",
    "generate:types": "ts-node ./scripts/generate-types.ts",
    "generate:utils": "ts-node ./scripts/generate-utils.ts",
    "generate:components": "ts-node ./scripts/generate-components.ts",
    "generate:indexes": "npm run generate:types && npm run generate:utils && npm run generate:components && npm run generate:stories",
    "generate:doc-css-vars": "ts-node ./scripts/doc-css-vars.ts",
    "generate:stories": "npm run generate:defaults && ts-node ./scripts/generate-stories.ts",
    "bundle": "npm run generate:indexes && tsc -b && vite build",
    "bundle:watch": "vite build --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "glob": "^11.0.3",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.32.0",
    "@storybook/addon-a11y": "^9.1.3",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-vitest": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@types/glob": "^9.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.3",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "ts-auto-mock": "^3.7.4",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "url": "^0.11.4",
    "vite": "^7.1.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
