{
  "name": "@jctrejosi/components-seed",
  "version": "1.0.2",
  "type": "module",
  "author": "Juan Carlos Trejos Iglesias",
  "main": "./bundle/index.cjs.js",
  "module": "./bundle/index.es.js",
  "types": "./bundle/index.d.ts",
  "files": [
    "bundle",
    ".prettierrc",
    "eslint.config.js"
  ],
  "eslintConfig": {
    "extends": "./eslint.config.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:fix": "eslint . --fix",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "format": "prettier --write .",
    "generate:types": "ts-node ./scripts/generate-types.ts",
    "generate:utils": "ts-node ./scripts/generate-utils.ts",
    "generate:components": "ts-node ./scripts/generate-components.ts",
    "generate:indexes": "npm run generate:types && npm run generate:utils && npm run generate:components && npm run generate:stories",
    "generate:stories": "ts-node ./scripts/generate-stories.ts",
    "generate:defaults": "ts-node ./scripts/generate-defaults.ts",
    "bundle": "npm run generate:indexes && npm run format && tsc -b && vite build",
    "watch": "vite build --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "glob": "13.0.0",
    "path": "0.12.7",
    "react-icons": "5.5.0"
  },
  "peerDependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/js": "9.39.2",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@types/glob": "9.0.0",
    "@types/node": "25.0.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react-swc": "4.2.2",
    "@vitest/browser": "4.0.17",
    "@vitest/coverage-v8": "4.0.17",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-storybook": "10.1.11",
    "globals": "17.0.0",
    "playwright": "1.57.0",
    "prettier": "3.8.0",
    "storybook": "10.1.11",
    "stylelint": "17.0.0",
    "stylelint-config-standard": "40.0.0",
    "stylelint-config-standard-scss": "17.0.0",
    "stylelint-order": "7.0.1",
    "ts-auto-mock": "3.7.4",
    "ts-morph": "27.0.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "url": "0.11.4",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.17"
  }
}
